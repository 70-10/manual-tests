    "@types/js-yaml": "4.0.9",
    "@types/node": "24.0.7",
    "@vitest/coverage-v8": "3.2.4",
    "typescript": "5.8.3",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.13.2",
